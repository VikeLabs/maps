{
    "info": {
        "title": "map uvic",
        "version": "0.0.1",
        "description": "An API for navigating around the University of Victoria."
    },
    "tags": [
    ],
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "The greatest server!"
        }
    ],
    "paths": {
        "/ping": {
            "get": {
                "summary": "Returns 200 OK and a small json object describing the server status.",
                "description": "A handy endpoint for checking to see weather the server is alive and replying.",
                "tags": [
                    ""
                ],
                "parameters": [
                ],
                "responses": {
                    "200": {
                        "description": "a successful ping!",
                        "content": {
                            "application/json": {
                                "example": {
                                    "success": true
                                },
                                "schema": {
                                    "$ref": "#/components/schemas/PingResponseBody"
                                }
                            }
                        }
                    }
                },
                "security": [
                ],
                "operationId": "getPing",
                "deprecated": false
            }
        },
        "/route": {
            "get": {
                "summary": "Route between two places.",
                "tags": [
                    ""
                ],
                "parameters": [
                    {
                        "schema": {
                            "type": "number"
                        },
                        "in": "query",
                        "name": "toLat",
                        "required": true,
                        "description": "The goal latitude."
                    },
                    {
                        "schema": {
                            "type": "number"
                        },
                        "in": "query",
                        "name": "toLon",
                        "required": true,
                        "description": "The goal longitude."
                    },
                    {
                        "schema": {
                            "type": "number"
                        },
                        "in": "query",
                        "name": "fromLat",
                        "required": true,
                        "description": "The initial latitude."
                    },
                    {
                        "schema": {
                            "type": "number"
                        },
                        "in": "query",
                        "name": "fromLon",
                        "required": true,
                        "description": "The initial longitude."
                    }
                ],
                "responses": {
                },
                "security": [
                ],
                "operationId": "getRoute",
                "deprecated": false
            }
        },
        "/search": {
            "get": {
                "summary": "searches the UVic campus based on a single search string",
                "description": "searches for buildings with a levenshteinDistance of 1 to the query string",
                "tags": [
                    ""
                ],
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "in": "query",
                        "name": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "a single result",
                        "content": {
                            "application/json": {
                                "example": {
                                    "results": [
                                        {
                                            "name": "Elliott Building",
                                            "center": {
                                                "latitude": 48.4627526,
                                                "longitude": -123.3108017
                                            }
                                        }
                                    ]
                                },
                                "schema": {
                                    "$ref": "#/components/schemas/SearchResponseBody"
                                }
                            }
                        }
                    }
                },
                "security": [
                ],
                "operationId": "getSearch",
                "deprecated": false
            }
        }
    },
    "components": {
        "schemas": {
            "PingResponseBody": {
                "properties": {
                    "success": {
                        "example": true,
                        "type": "boolean"
                    }
                },
                "example": {
                    "success": true
                },
                "type": "object",
                "required": [
                    "success"
                ]
            },
            "SearchResponseBody": {
                "properties": {
                    "results": {
                        "items": {
                            "$ref": "#/components/schemas/ResponseBodySearchResult"
                        },
                        "example": [
                            {
                                "name": "Elliott Building",
                                "center": {
                                    "latitude": 48.4627526,
                                    "longitude": -123.3108017
                                }
                            }
                        ],
                        "type": "array"
                    }
                },
                "example": {
                    "results": [
                        {
                            "name": "Elliott Building",
                            "center": {
                                "latitude": 48.4627526,
                                "longitude": -123.3108017
                            }
                        }
                    ]
                },
                "type": "object",
                "required": [
                    "results"
                ]
            },
            "ResponseBodySearchResult": {
                "properties": {
                    "name": {
                        "example": "Elliott Building",
                        "type": "string"
                    },
                    "center": {
                        "$ref": "#/components/schemas/Coordinate"
                    }
                },
                "example": {
                    "name": "Elliott Building",
                    "center": {
                        "latitude": 48.4627526,
                        "longitude": -123.3108017
                    }
                },
                "type": "object",
                "required": [
                    "center",
                    "name"
                ]
            },
            "Coordinate": {
                "properties": {
                    "latitude": {
                        "example": 48.4627526,
                        "type": "number"
                    },
                    "longitude": {
                        "example": -123.3108017,
                        "type": "number"
                    }
                },
                "example": {
                    "latitude": 48.4627526,
                    "longitude": -123.3108017
                },
                "type": "object",
                "required": [
                    "latitude",
                    "longitude"
                ]
            }
        },
        "securitySchemes": {
        }
    },
    "openapi": "3.0.0"
}